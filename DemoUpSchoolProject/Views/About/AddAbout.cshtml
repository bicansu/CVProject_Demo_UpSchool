@model  DemoUpSchoolProject.Models.Entities.TblAbout2
@{
    ViewBag.Title = "AddAbout";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<form method="post" enctype="multipart/form-data">
    @*fotoğraf eklemek istediğinde enctype eklenmelidir.*@
    <label>Ad Soyad</label>
    @Html.TextAreaFor(x => x.NameSurname, new { @class = "form-control" })
    <br />
    <label>Başlık</label>
    @Html.TextAreaFor(x => x.Title, new { @class = "form-control" })
    <br />
    <label>Açıklama</label>
    @Html.TextAreaFor(x => x.Description, new { @class = "form-control" })
    <br />
    <label>Image Url</label>
    <input type="file" name="ImageUrl"  class="form-control" />
    <br />
    <label>Image Url2</label>
    <input type="file" name="ImageUrl2"  class="form-control" />
    <br />
    <button class="btn btn-primary">Kaydet</button>

</form>
